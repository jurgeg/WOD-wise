{
  "expo": {
    "name": "WODwise",
    "slug": "wodwise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wodwise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wodwise.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "WODwise needs camera access to photograph your gym's whiteboard",
        "NSPhotoLibraryUsageDescription": "WODwise needs photo library access to select WOD images",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.wodwise.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "WODwise needs access to your photos to select WOD images",
          "cameraPermission": "WODwise needs camera access to photograph your gym's whiteboard"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "609db54b-d6fc-4e54-bb74-1f5157a5fc46"
      },
      "router": {}
    }
  }
}
